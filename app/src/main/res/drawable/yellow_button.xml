<?xml version="1.0" encoding="UTF-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <layer-list>
            <item>
                <shape>
                    <solid android:color="#fceb57"/>
                    <corners android:radius="4dp" />
                </shape>
            </item>
            <item android:bottom="4dp" android:left="4dp" android:right="4dp" android:top="4dp">
                <shape>
                    <stroke android:color="@color/white" android:width="1dp" android:dashGap="5dp" android:dashWidth="5dp" />
                    <corners android:radius="4dp" />
                    <padding android:bottom="5dp" android:left="5dp"
                        android:right="5dp" android:top="5dp" />
                </shape>
            </item>


        </layer-list>
    </item>
</selector>